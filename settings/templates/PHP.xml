        <variable name="INDEX" expression="" defaultValue="i" alwaysStopAt="true"/>
        <variable name="ARRAY" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="PHP" value="true"/>
        </context>
    </template>
    <template name="fori" value="for ($i=0; $i&lt;$len; $i++)&#10;{&#10;    $end$&#10;}"
